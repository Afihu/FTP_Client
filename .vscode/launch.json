{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch FTP Client",
            "request": "launch",
            "mainClass": "com.ftpclient.Main",
            "projectName": "FTP_Client",
            "vmArgs": "--module-path \"${workspaceFolder}\\lib\" --add-modules javafx.controls,javafx.fxml -Dprism.order=sw"
        }
    ]
}